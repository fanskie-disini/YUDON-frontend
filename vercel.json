{
    "version": 2,
    "builds": [{
        "src": "package.json",
        "use": "@vercel/static-build"
    }],
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "env": {
        "REACT_APP_API_URL": "https://yudon-backend-production.up.railway.app"
    },
    "rewrites": [{
        "source": "/(.*)",
        "destination": "/index.html"
    }]
}
  